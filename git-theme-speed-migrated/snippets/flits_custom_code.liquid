											<span class="flits-label">{{ order_fulfillmentStatusText | strip }}:</span>
											<span class="flits-value">{{ order.fulfillment_status_label }}</span>
											{% endif %}
										</div>
									</div>
									<div class="flits-text-right">
										<div class="flits-order-row">
											<span class="flits-label">{{ order_totalItemText | strip }}:</span>
											<span class="flits-value">{{ totalItemQuantity }}</span>
										</div>
