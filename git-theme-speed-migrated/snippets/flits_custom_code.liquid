										<span class="flits-value">{{ order.name | strip }}</span>
                                      
                                      <span class="flits-order-cancel-label {% if order.cancelled  %}{% else %}flits-d-none{% endif %}">Cancelled</span>
                                      {% for tag in order.tags %} 
                                      {% if tag  == 'Courier Return' or tag == 'Exchange' or tag == 'Return' %}
                                      <span class="flits-order-tags-label"> Returned </span>
                                      {% endif %}
                                      {% if tag contains 'request' and order.cancelled_at == null %}
                                      <span class="flits-order-cancel-request-label"> Cancellation Requested </span>
                                      {% endif %}
                                      {% endfor %}
									</div>
									<div class="flits-order-row">
										<span class="flits-label">{{ order_grandTotalText | strip }}:</span>
										<span class="flits-value" data-value='{{ order.total_price }}'>{{ order.total_price | money_without_trailing_zeros }}</span>
