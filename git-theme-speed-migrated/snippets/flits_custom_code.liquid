			{% render 'flits_capture_code' with 'component',template:'comp_pagination', attr:'data-flits-pagination="flits-order"' %}
		</div>
		{% render 'flits_capture_code' with 'component',template:'comp_order_card_skeleton_loader', class:'flits-order-loader' %}
