                                                          {% render 'flits-custom-warranty-drawer' with product: someProduct, orderId: order.id, itemID: item.id %}
                                                          </div>
                                                      {% endif %}
                                                  	{% endunless %}
                                                  {% endif %}
                                                 <!-- Warrenty fetach by metafield end -->

                                                  
													{% unless item.variant.id %}
														<p>{{ order_productNotExistText | strip }}</p>
													{% endunless %}
												</div>
											</div>
											{% assign order_line_item_price = order_line_item_price | plus: item.line_price %}
										{% endfor %}
									</div>
								</div>
								<div class="flits-order-row flits-order-action-row flits-text-right" 
                                     data-flits-order-created="{{ order.created_at | date: '%Y-%m-%d %H:%M:%S' }}"
                                     data-flits-order-id="{{ order.id }}"
                                     >
                                  {% comment %}
									{% if shop.metafields.Flits.IS_ORDER_CONTACT_US_ENABLE and shop.metafields.Flits.IS_ORDER_CONTACT_US_ENABLE == 1 %}
										{% render 'flits_capture_code' with 'component', template:'contact_us_button', customer:customer, order:order class:'flits-secondary-btn'%}
									{% endif %}
                                  {% endcomment %}
                                  
                                   {% comment %}
                                   <a href="https://help.headphonezone.in/support/tickets/new" class="flits-secondary-btn flits-mt-10 flits-button flits-headphone-zone-contact-us" target="_blank">Contact Us</a>
								   {% endcomment %}
                          
                                  {% unless vendorEventExper %}
                                 
                                    {% if order.fulfillment_status == "fulfilled" and order.cancelled_at == null  %}
                                      {% if order.tags contains "delivered" %}
                                       	{% unless order.tags contains 'flits-hide-return-button' %}                                       
  		                                  <a target="_blank" href="https://www.headphonezone.in/apps/return_prime" class="flits-button flits-secondary-btn flits-mt-10">Return Order</a>
  	   	                                {% endunless %}
                                         {% endif %}
                                    {% endif %}

                                  {% comment %} Order Cancle Button {% endcomment %}
                                    {% if order.fulfillment_status == "fulfilled" and order.cancelled_at == null %}
                                      {% if order.tags contains "confirmed" or order.tags contains "in-transit" %}
                                      {% comment %} Order Cancle Button With Pop Up Information{% endcomment %}
                                        <button type="button" class="flits-button flits-secondary-btn flits-mt-10 flits-order-cancel">
                                         {{ orderCancelButton | strip }}
                                        </button>
                                      {% endif %}
                                    {% elsif order.cancelled_at == null %}
                                       {% comment %} 
                                          Regular Order Cancle Button
                                          {% render 'flits_marmeto_cancel_order_button' with order:order,isIncludedFromFlitsAccountPage:true %}
                                       {% endcomment %}

                                      {% assign order_created_at = order.created_at | date: "%s" %}
                                      {% assign current_time = 'now' | date: "%s" %}
                                      {% assign time_difference = current_time | minus: order_created_at %}
                                      {% assign delay_in_seconds = 15 | times: 60 %}
                                      {% assign time_remaining = delay_in_seconds | minus: time_difference %}

                                      {% if time_remaining <= 0 %}
                                        <script>console.log("flits append")</script>
                                        {% render 'flits-cancel-order-new-popup' with order: order %}
                                      {% else %}
                                        {% render 'flits-cancel-timer-popup' with time_remaining: time_remaining, order: order %}
                                      {% endif %}
              
                                    {% endif %}
                                    {% endunless %}
                                  
                                  {% if order.fulfillment_status == "fulfilled" and order.cancelled_at == null %}
                                    {% comment %}<a target="_blank" href="https://track.headphonezone.in/my-order?order_id={{ order.name | replace:'#','' }}" class="flits-btn-inline flits-btn-default flits-btn-custom flits-btn flits-track-order">{{ 'flits.buttons.order_track' | t }}</a>{% endcomment %}
                                    {% if order.tags contains "delivered" %}
                                    {% else %}
                                      <a target="_blank" href="https://track.headphonezone.in/?waybill={{ tracking_number |replace:"Bluedart - ","" }}" class="flits-secondary-btn flits-mt-10 flits-button flits-track-order">{{ 'flits.buttons.order_track' | t }}</a>   
                                    {% endif %}
                                  {% endif %}
                                  
                                  
									{% render 'flits_capture_code' with 'component',template:'comp_button',
										type:'button',
										text:order_showOrderDetailsText,
										class:'flits-secondary-btn flits-mt-10 flits-order-details-btn'
									%}
                                  
                                  {% if order.fulfillment_status == "fulfilled" and order.cancelled_at == null %}
                                  <a target="_blank" href="/apps/{{ shop.metafields.Flits.APP_PROXY }}/api/{{ shop.metafields.Flits.APP_ID }}/{{ shop.metafields.Flits.SHOP_ID }}/generateInvoice?token={{ shop.metafields.Flits.SHOP_TOKEN }}&orderId={{order.id}}" class="flits-button flits-secondary-btn flits-mt-10">{{ 'flits.order_page.download_invoice' | t }}</a>                                    
                                  {% endif %}
                                  
                                  {% comment %}
									{% if shop.metafields.Flits.IS_REORDER_ENABLE and shop.metafields.Flits.IS_REORDER_ENABLE == 1 %}
										
										{% if order.line_items.size !=  countTotal  %}
										{% assign isDisable = '' %}
										{% endif %}
										{% assign dataTarget = order.id | prepend: 'data-target=' | append: " " | append: isDisable %}
										{% render 'flits_capture_code' with 'component',template:'comp_button',
											type:'button',
											text:order_reorderText,
											attr: dataTarget,
											class:'flits-primary-btn flits-mt-10 flits-reorder-btn'
										%} 
									{% endif %}
                                  {% endcomment %}
								</div>
								<div class="flits-order-details-summary">
									<div class="flits-order-row flits-address-row">
										<div class="flits-address-section">
											<div class="flits-shipping-address flits-address-box">
												<p class="flits-address-label flits-title">{{ order_shippingAddressText | strip }}</p>
												{{ order.shipping_address | format_address }}
											</div>
											<div class="flits-shipping-address flits-address-box">
												<p class="flits-address-label flits-title">{{ order_billingAddressText | strip }}</p>
												{{ order.billing_address | format_address }}
											</div>
										</div>
									</div>
									<div class="flits-order-row flits-cost-breakup-row">
										<div class="flits-order-cost-breakup">
											<p class="flits-title">{{ order_costBreakupText | strip }}</p>
											<div class="flits-order-row flits-order-subtotal">
												<span class="flits-label">{{ order_subtotalText | strip }}</span>
												<span class="flits-value">{{ order_line_item_price | money_without_trailing_zeros }}</span>
											</div>
											{% for discount in order.discounts %}
												<div class="flits-order-row flits-order-discount">
													<span class="flits-label">{{ order_discountCodeText | strip }}: <p class="flits-discount-truncate">{{ discount.code }}</P></span>
													<span class="flits-value">{{ discount.savings | money_without_trailing_zeros }}</span>
												</div>
											{% endfor %}
											{% for tax_line in order.tax_lines %}
												<div class="flits-order-row flits-order-tax">
													<span class="flits-label">{{ tax_line.title }} {{ tax_line.rate | times: 100 }}%</span>
													<span class="flits-value">{{ tax_line.price | money_without_trailing_zeros }}</span>
												</div>
											{% endfor %}
											<div class="flits-order-row flits-order-shipping-price">
												<span class="flits-label">{{ order_shippingAndHandlingText | strip }}</span>
