															{{ item.title | strip }}
														</a>
													{% else %}
														<a href="javascript:void(0);" class="flits-link flits-item-title" data-tippy-content="{{ order_productNotExistText | strip }}">
															{{ item.title | strip }}
														</a>
													{% endif %}
													<p class="flits-item-price">{{ item.price | money_without_trailing_zeros }}</p>
                                                  
                                                  <!--     Start :  Order Tracking Number -->
                                                  {% if item.fulfillment and item.fulfillment.tracking_number != blank %}
                                                  {% assign tracking_number  = item.fulfillment.tracking_number %}
