												<span class="flits-label">{{ order_paymentTypeText | strip }}</span>
												<span class="flits-value">{{ order.transactions[0].gateway | replace: '_',' ' | capitalize }}</span>
											</div>
										</div>
									</div>
								</div>
							</div> 
						{% endfor %}
					{% endpaginate %}
				{% endif %}
