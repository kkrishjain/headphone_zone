    "65c8133d-de96-4e48-bf84-892d45c72b3d": {
      "type": "image-with-text-overlay",
      "blocks": {
        "template--16031262376124__65c8133d-de96-4e48-bf84-892d45c72b3d-16838741340ca733b0-1": {
          "type": "heading",
          "settings": {
            "text": "Staggering Battery Life For Uninterrupted Playback",
            "heading_tag": "h3"
          }
        },
        "template--16031262376124__65c8133d-de96-4e48-bf84-892d45c72b3d-16838741340ca733b0-2": {
          "type": "richtext",
          "settings": {
            "content": "<p>With longer battery life, you can immerse yourself in more of your music, with up to 25 Hours of 44.1kHz FLAC playback, up to 23 Hours of 96kHz FLAC High-Resolution Audio playback, or up to 22 hours with music apps.<br/></p>"
          }
        }
      },
      "block_order": [
