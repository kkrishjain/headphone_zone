    "09ca051a-7883-45c0-aba3-ab9aeb624bcd": {
      "type": "text-with-icons",
      "blocks": {
        "template--16031262376124__09ca051a-7883-45c0-aba3-ab9aeb624bcd-1683880013777c7598-0": {
          "type": "item",
          "settings": {
            "icon": "picto-coupon",
            "custom_icon": "shopify://shop_images/Headphone-Zone-360-Audio-Logo.png",
            "mobile_icon_width": 120,
            "icon_width": 96,
            "icon_background_type": "none",
            "icon_background": "",
            "icon_color": "",
            "title": "",
            "content": "<p><strong>360 Reality Audio</strong><br/>360 Reality Audio is a new immersive music experience using Sony’s 360 Spatial Sound technology. </p>"
          }
        },
        "template--16031262376124__09ca051a-7883-45c0-aba3-ab9aeb624bcd-1683880013777c7598-1": {
          "type": "item",
          "settings": {
            "icon": "picto-coupon",
            "custom_icon": "shopify://shop_images/Headphone-Zone-MQA-Logo.png",
            "mobile_icon_width": 120,
            "icon_width": 100,
            "icon_background_type": "none",
            "icon_background": "",
            "icon_color": "",
            "title": "",
            "content": "<p><strong>MQA</strong><br/>The Sony NW ZX-707 Walkman includes MQA technology, which enables you to play back MQA audio files.</p>"
          }
        },
        "template--16031262376124__09ca051a-7883-45c0-aba3-ab9aeb624bcd-1683880013777c7598-2": {
          "type": "item",
          "settings": {
            "icon": "picto-coupon",
            "custom_icon": "shopify://shop_images/Headphone-ZOne-LDAC-Logo-PNG.png",
            "mobile_icon_width": 120,
            "icon_width": 100,
            "icon_background_type": "none",
            "icon_background": "",
            "icon_color": "",
            "title": "",
            "content": "<p><strong>Bluetooth V5.2</strong><br/>Enjoy Hi-Res Audio quality without wires thanks to LDAC, Sony's premium wireless codec. <br/></p>"
          }
        }
      },
